{
  "name": "back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build && npm run copy-assets && node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "copy-assets": "copyfiles -f -u 1 \"src/Modules/nodemailer/templates/**/*\" dist/Modules/nodemailer/templates",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register src/data-source.ts",
    "migration:generate": "npm run build && typeorm migration:generate -d dist/config/typeorm.config.js",
    "migration:run": "npm run build && typeorm migration:run -d dist/config/typeorm.config.js",
    "migration:revert": "npm run build && typeorm migration:revert -d dist/config/typeorm.config.js",
    "migration:create": "typeorm migration:create src/migrations/MigrationName",
    "seed:categories": "ts-node -r tsconfig-paths/register src/Modules/Categories/seeders/categories.seeder.ts",
    "seed:products": "ts-node -r tsconfig-paths/register src/Modules/Products/seeders/products.seeder.ts",
    "seed:plantations": "ts-node -r tsconfig-paths/register src/Modules/Plantations/seeders/plantations.seeder.ts",
    "seed:phenologies": "ts-node -r tsconfig-paths/register src/Modules/Phenologies/seeders/phenologies.seeder.ts",
    "seed:diseases": "ts-node -r tsconfig-paths/register src/Modules/Diseases/seeders/diseases.seeder.ts",
    "seed:applicationtype": "ts-node -r tsconfig-paths/register src/Modules/ApplicationTypes/seeders/applicationtypes.seeder.ts",
    "seed:applicationplan": "ts-node -r tsconfig-paths/register src/Modules/ApplicationPlans/seeders/applicationplans.seeder.ts",
    "run:category:seed": "ts-node -r tsconfig-paths/register src/Modules/Categories/seeders/categories.seeder.ts",
    "run:product:seed": "ts-node -r tsconfig-paths/register src/Modules/Products/seeders/products.seeder.ts ",
    "run:recommendation:seed": "ts-node -r tsconfig-paths/register src/Modules/Recomendations/seeders/recomendations.seeder.ts",
    "seed:all": "ts-node -r tsconfig-paths/register src/Modules/seed.ts",
    "email": "email dev --dir src/emails"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/core": "^11.1.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@react-email/components": "^0.5.1",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "buffer-to-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.7.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-openid-connect": "^2.19.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "nodemailer": "^7.0.5",
    "openai": "^5.12.2",
    "passport": "^0.7.0",
    "passport-auth0": "^1.4.4",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-email": "^4.2.8",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "stripe": "^18.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.5",
    "@nestjs/typeorm": "^11.0.0",
    "@react-email/preview-server": "^4.2.8",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.13.3",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.17.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-auth0": "^1.0.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.25",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
